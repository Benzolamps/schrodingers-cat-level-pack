  }
}

terminal when (Finish) {
  text: "TTRS:notrans=%e0"
  options: {
    "TTRS:notrans=Exit" next: Exit
  }
}

terminal when (Exit) {
  text: [[TTRS:TermDlg.CLI_Global.Ln0159.0.text.TerminatingLibrarySessionW1W1=
Terminating session%w1.%w1.%w1.%w1.
Session terminated.
]]
  exit
}
